{
    "window.titleBarStyle": "custom",
    "extensions.ignoreRecommendations": true,
    "git.openRepositoryInParentFolders": "always",
    "editor.minimap.enabled": false,
    "editor.scrollbar.horizontalScrollbarSize": 0,
    "editor.scrollbar.verticalScrollbarSize": 12,
    "editor.folding": false,
    "editor.foldingHighlight": false,
    "editor.guides.indentation": false,
    "workbench.layoutControl.enabled": false,
    "window.commandCenter": false,
    "terminal.integrated.tabs.hideCondition": "never",
    "editor.autoClosingBrackets": "never",
    "editor.bracketPairColorization.enabled": false,
    "terminal.integrated.commandsToSkipShell": [
        "-workbench.action.quickOpen",
        "workbench.action.debug.nextConsole",
    ],
    "editor.renderLineHighlight": "none",
    "python.languageServer": "Pylance",
    "git.confirmSync": false,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "explorer.excludeGitIgnore": true,
    "editor.autoClosingOvertype": "never",
    "editor.autoClosingDelete": "never",
    "editor.autoClosingQuotes": "never",
    "window.zoomLevel": 1,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "scm.diffDecorations": "none",
    "window.customTitleBarVisibility": "auto",
    "editor.cursorSurroundingLines": 12,
    "editor.acceptSuggestionOnCommitCharacter": true,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.hover.enabled": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestOnTriggerCharacters": false,
    "editor.wordBasedSuggestions": "off",
    "telemetry.telemetryLevel": "off",
    "rest-client.enableTelemetry": false,
    "editor.wordWrap": "on",
    "terminal.integrated.scrollback": 120015,
    "editor.fontSize": 14,
    "debug.console.fontSize": 14,
    "terminal.integrated.fontSize": 14,
    "chat.editor.fontSize": 14,
    "markdown.preview.fontSize": 14,
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            "*.mutable": {
                "underline": false,
            }
        }
    },
    "explorer.confirmDelete": false,
    "editor.wrappingIndent": "none",
    "editor.renderWhitespace": "trailing",
    "files.simpleDialog.enable": true,
    "files.watcherExclude": {
        "**/__pycache__/**": true
    },
    "search.exclude": {
        "**/__pycache__/**": true
    },
    "accessibility.verbosity.terminal": false,
    "workbench.activityBar.location": "bottom",
    "editor.semanticHighlighting.enabled": false,
    "debug.onTaskErrors": "abort",
    "editor.stickyScroll.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "editor.lineNumbers": "off",
    "rest-client.rememberCookiesForSubsequentRequests": false,
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "dotnet.codeLens.enableReferencesCodeLens": false,
    "workbench.editor.editorActionsLocation": "titleBar",
    "files.exclude": {
        "**/.env/": true
    },
    "workbench.editor.wrapTabs": true,
    "window.density.editorTabHeight": "compact",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "python.analysis.typeCheckingMode": "standard",
    "workbench.colorTheme": "One Monokai Shadow",
}